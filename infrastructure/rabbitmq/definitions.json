{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password_hash": "vL8Z/XXdv8DRtlEINaSPNjPHY3LmqGFWFqDGGHN3f9Y2mOa8",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "community-connect-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "message-ttl",
      "pattern": "^(service|notification|communication)\\.",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 86400000,
        "max-length": 10000
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "service.requests",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 1000
      }
    },
    {
      "name": "service.completed",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 1000
      }
    },
    {
      "name": "notification.email",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 5000
      }
    },
    {
      "name": "notification.push",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 5000
      }
    },
    {
      "name": "communication.messages",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000
      }
    },
    {
      "name": "transaction.credits",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 1000
      }
    }
  ],
  "exchanges": [
    {
      "name": "community.connect.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "service.exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "notification.exchange",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "community.connect.exchange",
      "vhost": "/",
      "destination": "service.requests",
      "destination_type": "queue",
      "routing_key": "service.request.*",
      "arguments": {}
    },
    {
      "source": "community.connect.exchange",
      "vhost": "/",
      "destination": "service.completed",
      "destination_type": "queue",
      "routing_key": "service.completed.*",
      "arguments": {}
    },
    {
      "source": "community.connect.exchange",
      "vhost": "/",
      "destination": "communication.messages",
      "destination_type": "queue",
      "routing_key": "communication.message.*",
      "arguments": {}
    },
    {
      "source": "community.connect.exchange",
      "vhost": "/",
      "destination": "transaction.credits",
      "destination_type": "queue",
      "routing_key": "transaction.credit.*",
      "arguments": {}
    },
    {
      "source": "notification.exchange",
      "vhost": "/",
      "destination": "notification.email",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "notification.exchange",
      "vhost": "/",
      "destination": "notification.push",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    }
  ]
}